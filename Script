Script3.Name = "Script3"


Script3.Parent = TeleportFrame


Script3.BackgroundColor3 = Color3.fromRGB(85, 255, 0)


Script3.BorderColor3 = Color3.fromRGB(0, 0, 0)


Script3.Position = UDim2.new(0.121621624, 0, 0.549763024, 0)


