Script3.Text = "Teleport"


Script3.TextColor3 = Color3.fromRGB(0, 0, 0)


Script3.TextSize = 14.000


Script3.MouseButton1Down:connect(function()


	local username = string.lower(Target.Text)


	local foundchild = false


	local child


	i,v in pairs(game.Players:GetChildren()) do


		if string.lower(v.name) == username then


			foundchild = true


			child = v


		end 


	end


	if foundchild = true then


		game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = child.Character.HumanoidRootPart.CFrame


	end


end)



